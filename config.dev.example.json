{
    "PROJECT_NAME": "MediaVault Manager",
    "VERSION": "0.1.0",
    "DESCRIPTION": "MediaVault Management System (Development)",
    "API_BASE_URL": "http://localhost:8000",
    "MEDIA_ROOT": "media",
    "ALLOWED_EXTENSIONS": ["jpg", "jpeg", "png", "gif", "mp4", "mp3"],
    "MAX_CONTENT_LENGTH": 16777216,
    "JELLYFIN": {
        "url": "http://localhost:8096",
        "api_key": "your-dev-api-key-here"
    },
    "MEDIA_MERGE": {
        "user": "1000",
        "group": "1000"
    },
    "MEDIA_LIBRARY": {
        "default_source_path": "./media",
        "source_matrix": {
            "movies": {
                "quality_order": ["uhd", "hd"],
                "merged_path": "./media/movies-merged"
            },
            "tv": {
                "quality_order": ["uhd", "hd"],
                "merged_path": "./media/tv-merged"
            }
        },
        "cache_path": "./media/cache",
        "cache_shadow_path": "./media/media-shadow-cache"
    }
} 